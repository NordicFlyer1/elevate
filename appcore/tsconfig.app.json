{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "baseUrl": "./",
    "types": [
      "chrome",
      "electron"
    ],
    "paths": {
      "fs": [
        "./modules/browser/null.ts"
      ],
      "@elevate/shared/constants/*": [
        "./modules/shared/constants/*"
      ],
      "@elevate/shared/enums/*": [
        "./modules/shared/enums/*"
      ],
      "@elevate/shared/data/*": [
        "./modules/shared/data/*"
      ],
      "@elevate/shared/electron/*": [
        "./modules/shared/electron/*"
      ],
      "@elevate/shared/sync/*": [
        "./modules/shared/sync/*"
      ],
      "@elevate/shared/models/*": [
        "./modules/shared/models/*"
      ],
      "@elevate/shared/resolvers/*": [
        "./modules/shared/resolvers/*"
      ],
      "@elevate/shared/exceptions/*": [
        "./modules/shared/exceptions/*"
      ],
      "@elevate/shared/tools/*": [
        "./modules/shared/tools/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/main.ts",
    "src/polyfills.ts"
  ]
}
